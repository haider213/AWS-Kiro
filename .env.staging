# Staging Environment Variables

# Backend API URL - Update with your staging backend URL
VITE_BACKEND_URL=https://staging-backend-domain.com

# Application Configuration
VITE_APP_NAME=RAG Pipeline Educator (Staging)
VITE_APP_VERSION=1.0.0-staging

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_MONITORING=true

# Staging Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# CDN Configuration
VITE_CDN_URL=https://staging-cdn-domain.com
VITE_ASSETS_URL=https://staging-cdn-domain.com/assets

# Analytics Configuration
VITE_ANALYTICS_ID=staging-analytics-id
VITE_MONITORING_DSN=staging-monitoring-dsn